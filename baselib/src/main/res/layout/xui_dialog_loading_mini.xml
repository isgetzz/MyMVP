<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialog_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/shape5f0"
    android:gravity="center"
    android:minWidth="@dimen/x90"
    android:minHeight="@dimen/x90"
    android:orientation="vertical"
    android:padding="@dimen/x10">

    <com.baselib.customView.MiniLoadingView
        android:id="@+id/mini_loading_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:mlv_loading_view_color="@color/gray999"
        app:mlv_loading_view_size="@dimen/x23" />

    <TextView
        android:id="@+id/tv_tip_message"
        style="@style/Gray6Text11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x10"
        android:text="@string/loading" />

</LinearLayout>